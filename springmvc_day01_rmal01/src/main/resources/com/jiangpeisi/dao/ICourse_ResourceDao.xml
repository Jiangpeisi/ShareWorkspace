<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiangpeisi.dao.ICourse_ResourceDao">
    <insert id="insert" parameterType="com.jiangpeisi.domain.Course_Resource">
        insert into course_resource (course_id,resource_url,resource_name,resource_type) values (#{course_id},#{resource_url},#{resource_name},#{resource_type});
    </insert>
    <select id="findById" parameterType="java.lang.Integer" resultType="com.jiangpeisi.domain.Course_Resource">
        select * from course_resource where id=#{id}
    </select>
</mapper>