<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiangpeisi.dao.IChoose_CourseDao">

    <insert id="insert" parameterType="com.jiangpeisi.domain.Course_Choose">
        insert into course_choose(course_offering_id,student_id) values(#{course_offering_id},#{student_id})
    </insert>
    <delete id="delete" parameterType="com.jiangpeisi.domain.Course_Choose">
        delete from course_choose where course_offering_id=#{course_offering_id} and student_id=#{student_id}
    </delete>
</mapper>