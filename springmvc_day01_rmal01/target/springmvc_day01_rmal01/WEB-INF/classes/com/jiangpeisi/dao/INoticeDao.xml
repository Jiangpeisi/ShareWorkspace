<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiangpeisi.dao.INoticeDao">
    <select id="findAll"  resultType="com.jiangpeisi.domain.Notice">
        select * from notice
    </select>
    <insert id="insert" parameterType="com.jiangpeisi.domain.Notice">
        insert into notice (title,content,release_time,announcer) values (#{title},#{content},#{release_time},#{announcer});
    </insert>
</mapper>