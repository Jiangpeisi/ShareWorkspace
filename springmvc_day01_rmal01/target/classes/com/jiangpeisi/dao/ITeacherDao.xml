<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiangpeisi.dao.ITeacherDao">
    <select id="findTeacher" parameterType="com.jiangpeisi.domain.Teacher" resultType="com.jiangpeisi.domain.Teacher">
        select * from teacher where username=#{username} and password=#{password}
    </select>
</mapper>